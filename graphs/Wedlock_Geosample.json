{
    "properties": {
        "name": "Wedlock Geosample",
        "id": "Wedlock_Geosample",
        "project": "geoflo-test",
        "environment": {
            "type": "noflo-nodejs"
        }
    },
    "inports": {},
    "outports": {},
    "groups": [
        {
            "name": "Paired Location Processing",
            "nodes": [
                "core/Drop_6mhxd",
                "core/MakeFunction_avpxs",
                "core/Output_nhqzn",
                "geoflow/LocationTracker_qsxis",
                "geoflow/StrayFromAlert_f2fgf",
                "geoflow/TangledPairTracker_d6dnx",
                "geoflow/UDPDataSensor_o5s0l",
                "objects/FilterPropertyValue_pylo7",
                "objects/FilterPropertyValue_wc73v",
                "strings/ParseJson_z6km6"
            ],
            "metadata": {
                "description": "Processes paired locations; providing alerts upon stray-from at a specified distance."
            }
        }
    ],
    "processes": {
        "geoflow/UDPDataSensor_o5s0l": {
            "component": "geoflow/UDPDataSensor",
            "metadata": {
                "label": "UDP Data Sensor",
                "x": 288,
                "y": 144
            }
        },
        "strings/ParseJson_z6km6": {
            "component": "strings/ParseJson",
            "metadata": {
                "label": "ParseJson",
                "x": 432,
                "y": 252
            }
        },
        "core/MakeFunction_avpxs": {
            "component": "core/MakeFunction",
            "metadata": {
                "label": "Create Geometry",
                "x": 612,
                "y": 252
            }
        },
        "core/Output_nhqzn": {
            "component": "core/Output",
            "metadata": {
                "label": "Console Output",
                "x": 1440,
                "y": 720
            }
        },
        "core/Drop_6mhxd": {
            "component": "core/Drop",
            "metadata": {
                "label": "Rubbish Bucket",
                "x": 1440,
                "y": 144
            }
        },
        "geoflow/LocationTracker_qsxis": {
            "component": "geoflow/LocationTracker",
            "metadata": {
                "label": "Track Unique Objects",
                "x": 792,
                "y": 252
            }
        },
        "objects/FilterPropertyValue_pylo7": {
            "component": "objects/FilterPropertyValue",
            "metadata": {
                "label": "New Tracked Object",
                "x": 1008,
                "y": 360
            }
        },
        "objects/FilterPropertyValue_wc73v": {
            "component": "objects/FilterPropertyValue",
            "metadata": {
                "label": "Tracked Object Moved",
                "x": 1008,
                "y": 504
            }
        },
        "geoflow/TangledPairTracker_d6dnx": {
            "component": "geoflow/TangledPairTracker",
            "metadata": {
                "label": "Tangled Pair Tracker",
                "x": 1224,
                "y": 504
            }
        },
        "geoflow/StrayFromAlert_f2fgf": {
            "component": "geoflow/StrayFromAlert",
            "metadata": {
                "label": "Stray-From Geo. Alert",
                "x": 1224,
                "y": 720
            }
        }
    },
    "connections": [
        {
            "src": {
                "process": "strings/ParseJson_z6km6",
                "port": "out"
            },
            "tgt": {
                "process": "core/MakeFunction_avpxs",
                "port": "in"
            },
            "metadata": {
                "route": 7
            }
        },
        {
            "src": {
                "process": "geoflow/UDPDataSensor_o5s0l",
                "port": "log"
            },
            "tgt": {
                "process": "core/Drop_6mhxd",
                "port": "in"
            },
            "metadata": {
                "route": 3
            }
        },
        {
            "src": {
                "process": "core/MakeFunction_avpxs",
                "port": "out"
            },
            "tgt": {
                "process": "geoflow/LocationTracker_qsxis",
                "port": "in"
            },
            "metadata": {
                "route": 7
            }
        },
        {
            "src": {
                "process": "geoflow/LocationTracker_qsxis",
                "port": "log"
            },
            "tgt": {
                "process": "core/Drop_6mhxd",
                "port": "in"
            },
            "metadata": {
                "route": 3
            }
        },
        {
            "src": {
                "process": "geoflow/LocationTracker_qsxis",
                "port": "out"
            },
            "tgt": {
                "process": "objects/FilterPropertyValue_pylo7",
                "port": "in"
            },
            "metadata": {
                "route": 4
            }
        },
        {
            "src": {
                "process": "geoflow/LocationTracker_qsxis",
                "port": "out"
            },
            "tgt": {
                "process": "objects/FilterPropertyValue_wc73v",
                "port": "in"
            },
            "metadata": {
                "route": 9
            }
        },
        {
            "src": {
                "process": "geoflow/UDPDataSensor_o5s0l",
                "port": "out"
            },
            "tgt": {
                "process": "strings/ParseJson_z6km6",
                "port": "in"
            },
            "metadata": {
                "route": 7
            }
        },
        {
            "src": {
                "process": "objects/FilterPropertyValue_pylo7",
                "port": "out"
            },
            "tgt": {
                "process": "geoflow/TangledPairTracker_d6dnx",
                "port": "in"
            },
            "metadata": {
                "route": 4
            }
        },
        {
            "src": {
                "process": "objects/FilterPropertyValue_wc73v",
                "port": "out"
            },
            "tgt": {
                "process": "geoflow/TangledPairTracker_d6dnx",
                "port": "in"
            },
            "metadata": {
                "route": 9
            }
        },
        {
            "src": {
                "process": "geoflow/TangledPairTracker_d6dnx",
                "port": "log"
            },
            "tgt": {
                "process": "core/Drop_6mhxd",
                "port": "in"
            },
            "metadata": {
                "route": 3
            }
        },
        {
            "src": {
                "process": "geoflow/TangledPairTracker_d6dnx",
                "port": "out"
            },
            "tgt": {
                "process": "geoflow/StrayFromAlert_f2fgf",
                "port": "in"
            },
            "metadata": {
                "route": 1
            }
        },
        {
            "src": {
                "process": "geoflow/StrayFromAlert_f2fgf",
                "port": "log"
            },
            "tgt": {
                "process": "core/Output_nhqzn",
                "port": "in"
            },
            "metadata": {
                "route": 3
            }
        },
        {
            "src": {
                "process": "geoflow/StrayFromAlert_f2fgf",
                "port": "out"
            },
            "tgt": {
                "process": "core/Output_nhqzn",
                "port": "in"
            },
            "metadata": {
                "route": 10
            }
        },
        {
            "data": "33333",
            "tgt": {
                "process": "geoflow/UDPDataSensor_o5s0l",
                "port": "port"
            }
        },
        {
            "data": "x.geometry={x:x.lon,y:x.lat};return x;",
            "tgt": {
                "process": "core/MakeFunction_avpxs",
                "port": "function"
            }
        },
        {
            "data": "geometry",
            "tgt": {
                "process": "geoflow/LocationTracker_qsxis",
                "port": "geom_field"
            }
        },
        {
            "data": "device_id",
            "tgt": {
                "process": "geoflow/LocationTracker_qsxis",
                "port": "id_field"
            }
        },
        {
            "data": "type=\"new_track\"",
            "tgt": {
                "process": "objects/FilterPropertyValue_pylo7",
                "port": "accept"
            }
        },
        {
            "data": "type=\"location_change\"",
            "tgt": {
                "process": "objects/FilterPropertyValue_wc73v",
                "port": "accept"
            }
        },
        {
            "data": "id",
            "tgt": {
                "process": "geoflow/TangledPairTracker_d6dnx",
                "port": "id_field"
            }
        },
        {
            "data": "geometry",
            "tgt": {
                "process": "geoflow/StrayFromAlert_f2fgf",
                "port": "geom_field"
            }
        },
        {
            "data": "2.5",
            "tgt": {
                "process": "geoflow/StrayFromAlert_f2fgf",
                "port": "alarm_distance"
            }
        }
    ]
}